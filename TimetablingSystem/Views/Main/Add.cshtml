@{
    ViewBag.Title = "Add";
}

<div class="row">
  <div class="col-md-2"></div>
  <div class="col-md-4">
  holy spacing batman
        <div id="propertiesBox">
            <select id="modTitleSelect" name="modTitleSelect" class="modChooser" onchange="ModuleSelector(this)"></select></br>
            <select id="modCodeSelect" name="modCodeSelect" class="modChooser" onchange="ModuleSelector(this)"></select></br>
            <table class='table reqTable'>
                <tr>
                    <td><input type='checkbox' class='specReq' id='QUR' onchange='facilityStore()'><label for='QUR'>Quality Room</label></td>
                    <td><input type='checkbox' class='specReq' id='WHC' onchange='facilityStore()'><label for='WHC'>Wheelchair</label></td>
                </tr>
                <tr>
                    <td><input type='checkbox' class='specReq' id='DP1' onchange='facilityStore()'><label for='DP1'>Data Projector</label></td>
                    <td><input type='checkbox' class='specReq' id='DP2' onchange='facilityStore()' disabled='true'><label for='DP2'>(Second) Data Projector</label></td>
                </tr>
                <tr>
                    <td><input type='checkbox' class='specReq' id='VIS' onchange='facilityStore()'><label for='VIS'>Visualiser</label></td>
                    <td><input type='checkbox' class='specReq' id='VDB' onchange='facilityStore()'><label for='VDB'>Video/DVD/BluRay</label></td>
                </tr>
                <tr>
                    <td><input type='checkbox' class='specReq' id='CMP' onchange='facilityStore()'><label for='CMP'>Computer</label></td>
                    <td><input type='checkbox' class='specReq' id='WHB' onchange='facilityStore()'><label for='WHB'>Whiteboard</label></td>
                </tr>
                <tr>
                    <td><input type='checkbox' class='specReq' id='CHB' onchange='facilityStore()'><label for='CHB'>Chalkboard</label></td>
                    <td><input type='checkbox' class='specReq' id='NER' onchange='facilityStore()'><label for='NER'>Near Previous Room</label></td>
                </tr>
                <tr>
                    <td>No of Students:</td><td><input type='textbox' class='specReqText' id='CAP' value='50' onchange='facilityStore()'></td>
                </tr>
                <tr>
                    <td>Park:</td>
                    <td>
                        <select id='PRK' onchange='facilityStore()' class='modChooser'>
                            <option value='ANY' selected>Any</option>
                            <option value='E'>East</option>
                            <option value='C'>Central</option>
                            <option value='W'>West</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td>Other Requirements:</td>
                    <td><input type='textbox' class='specReqText' onchange='facilityStore()' id='ORE' placeholder='Type here...'></td>
                </tr>
                <tr><td></td><td><label id='charToGo'> </label></td></tr>
                <tr><td>Number of rooms:</td><td><select id='NOR'><option value='1'>1</option><option value='2'>2</option><option value='3'>3</option><option value='4'>4</option><option value='5'>5</option></select></td></tr>
                <tr>
                    <td>Priority:</td><td>
                        <input type='radio' class='specReqP' id='PRY' name='Priority' ><label for='PRY'>Yes</label>
                        <input type='radio' class='specReqP' id='PRN' name='Priority' ><label for='PRN'>No</label></td>
                </tr>
            </table>
        </div>
        <div id="weekTable">
        </div>
  </div>
  <div class="col-md-4">this will be the filtered rooms list blah blah blah</div>
  <div class="col-md-2"></div>
</div>

@Scripts.Render("~/bundles/addRequests")